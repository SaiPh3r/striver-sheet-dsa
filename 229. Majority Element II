class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        int n = nums.size();
        vector<int>ans;
        unordered_map<int,int>map;
        for(int i=0;i<nums.size();i++){
            if(map.find(nums[i]) != map.end()){
                // this means milgya
                map[nums[i]]++;
            }else{
                map[nums[i]] = 1;
            }
        }
        for(auto &i : map){
            if(i.second > n/3){
                ans.push_back(i.first);
            }
        }
        return ans;
    }
};