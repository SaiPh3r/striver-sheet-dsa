class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int i=0;
        int ans =0;
        int count = 0;
        while(i<nums.size()){
            if(nums[i] == 1){
                count++;
                i++;
            }else{
                ans = max(count,ans);
                count = 0;
                i++;
            }
        }
        ans = max(ans, count);
        return ans;
    }
};