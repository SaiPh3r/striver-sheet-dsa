class Solution {
public:
 // approach -> from 1th element if we sell then we nust but on min before that
    int maxProfit(vector<int>& prices) {
        int ansprofit = 0;
        int mini = prices[0];
        for(int i = 0; i<prices.size();i++){
            int currentProfit = prices[i] - mini;
            ansprofit = max(currentProfit , ansprofit);
            mini = min(mini , prices[i]);
        }
        return ansprofit;
        
    }
};